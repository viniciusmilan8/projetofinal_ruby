<h1>Completed Tasks</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Date Conclusion</th>
    </tr>
  </thead>

  <tbody>
    <% @completed_tasks.each do |task| %>
      <tr>
        <td><%= task.title %></td>
        <td><%= task.description %></td>
        <td><%= task.dateconclusion %></td>
      </tr>
    <% end %>
  </tbody>
</table>
